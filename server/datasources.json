{
  "elasticsearch-plain": {
    "name": "elasticsearch-plain",
    "connector": "invenioConnector",
    "version": 7,
    "index": "rdmrecords-records-record-v4.0.0*",
    "hosts": [
      {
        "protocol": "http",
        "host": "localhost",
        "port": 9200
      }
    ],
    "apiVersion": "6.5",
    "refreshOn": ["save","create", "updateOrCreate"],
    "log": "trace",
    "mappingProperties": {
      "pid": {
        "type": "date",
        "index": true
      }
    }
  }
}
